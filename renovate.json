{
 "$schema": "https://docs.renovatebot.com/renovate-schema.json",
 "labels": ["dependencies"],
 "extends": [
  "config:base"
 ],
 "packageRules": [
  {
   "matchPackagePatterns": [
    "*"
   ],
   "matchUpdateTypes": [
    "minor",
    "patch"
   ],
   "groupName": "all non-major dependencies",
   "groupSlug": "all-minor-patch"
  }
 ]
}
